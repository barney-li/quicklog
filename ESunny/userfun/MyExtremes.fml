//--------------------------------------------------------------
// 简称: MyExtremes
// 名称: MyExtremes
// 类型: 用户函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	NumericSeries price;
	Bool maxFlag;
	Numeric length;
	Numeric offset(0);
Vars
	Numeric retVal(0);
	Numeric i(0);
Begin

	If(CurrentBar >= length+offset-1)
	{
		If(maxFlag)
		{
			For i=offset to length+offset-1
			{
				If(price[i]>retVal)
				{
					retVal = price[i];
				}
			}
		}
		Else
		{
			For i=offset to length+offset-1
			{
				If(price[i]<retVal)
				{
					retVal = price[i];
				}
			}
		}
	}
	Return retVal;
End
