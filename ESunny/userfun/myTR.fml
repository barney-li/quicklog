//--------------------------------------------------------------
// 简称: myTR
// 名称: myTR
// 类型: 用户函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	
Vars
	NumericSeries returnTR;
	NumericArray tempTR;
	NumericArray tempTR1;
Begin
	ArrAdd(tempTR, Abs(High - Low));
	ArrAdd(tempTR, Abs(High - Close[1]));
	ArrAdd(tempTR, Abs(Low - Close[1]));
	tempTR1 = ArrSort(tempTR, False);
	returnTR = tempTR1[0];
	Return returnTR;
End
