//--------------------------------------------------------------
// 简称: KDJ
// 名称: KDJ
// 类型: 内建函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	Numeric N(9);
	NumericRef RSV;
	NumericRef KValue;
	NumericRef DValue;
	NumericRef JValue;
Vars
	Numeric Num;
Begin
	RSV = (CLOSE-LowestFC(LOW,N))/(HighestFC(HIGH,N)-LowestFC(LOW,N))*100;
	KValue = SMA(RSV,3,1); 
	DValue = SMA(KValue,3,1);
	JValue = 3*KValue-2*DValue;
	Return RSV;
End
