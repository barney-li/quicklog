//--------------------------------------------------------------
// 简称: Lowest
// 名称: 求最低
// 类型: 用户函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	NumericSeries Price(0);
	Numeric Length(5); 
Vars
	Numeric LowestValue;
	Numeric i;
Begin
	LowestValue = Price;	
	for i=1 to Length - 1
    {
		If(Price[i] < LowestValue)
			LowestValue = Price[i];
	}
	Return LowestValue;
End