//--------------------------------------------------------------
// 简称: LowestBar
// 名称: 求最低只出现的Bar
// 类型: 用户函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	NumericSeries Price(0);
	Numeric Length(5); 
Vars
	Numeric LowestValue;
	Numeric i;
	Numeric reBar(0);
Begin
	LowestValue = Price;	
	for i=1 to Length - 1
    {
		If(Price[i] < LowestValue)
		{
			LowestValue = Price[i];
			reBar = i;
		}
	}
	Return reBar;
End
