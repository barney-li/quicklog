//--------------------------------------------------------------
// 简称: PercentChange
// 名称: 求涨跌幅
// 类型: 用户函数
// 输出: Numeric
//--------------------------------------------------------------
Params
	NumericSeries Price(1);
	Numeric Length(10);
Vars
	Numeric PCValue(0);
Begin
	If (Price[Length] != 0 )
	{
		PCValue = ( Price - Price[Length] ) / Price[Length];
	}Else
	{
		PCValue = 0;
	}
	Return PCValue;
End
