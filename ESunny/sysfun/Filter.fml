//--------------------------------------------------------------
// 简称: Filter
// 名称: 条件过滤
// 类型: 用户函数
// 输出: Bool
//--------------------------------------------------------------
Params
	Bool Con;
	Numeric N;
Vars
	BoolSeries ConSeries;
Begin
	Bool RealCon;
	if(Con)
	{
		RealCon = True;
		Integer i;
		for i=0 to N
		{
			if(ConSeries[i])
			{
				RealCon = False;
				Break;		
			}
		}
	}	
	Else
		RealCon = False;
		
	ConSeries = RealCon;
	
	Return RealCon;
End
